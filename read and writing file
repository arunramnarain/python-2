# open file in writemode
file=open("example.txt","w")
#write to the file
file.write("Hello,world.\n")
file.write("I enjoy coding")
file.close()

#open file (read only)
file=open("example.txt","r")
#read the entire content
content=file.read()
print("File Content:\n"+content)
file.close()
print("-"*32)
